{
  "name": "billing/commands",
  "description": "Billing module commands",
  "keywords": [
    "billing",
    "commands"
  ],
  "homepage": "https://wemx.net",
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "GIGABAIT",
      "email": "xgigabaitx@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^7.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Billing\\Commands\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Billing\\Commands\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Billing\\Commands\\CommandsServiceProvider"
      ],
      "aliases": {
        "Commands": "Billing\\Commands\\CommandsFacade"
      }
    }
  }
}